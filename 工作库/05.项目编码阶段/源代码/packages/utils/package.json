{
  "name": "fusion-utils",
  "version": "0.0.1",
  "main": "lib",
  "module": "esm",
  "umd:main": "dist/fusion.path.umd.production.js",
  "unpkg": "dist/fusion.path.umd.production.js",
  "jsdelivr": "dist/fusion.path.umd.production.js",
  "jsnext:main": "esm",
  "types": "esm/index.d.ts",
  "engines": {
    "npm": ">=3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf -rf lib esm dist && npm run build:cjs && npm run build:esm && npm run build:umd",
    "build:cjs": "tsc --project tsconfig.build.json",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --outDir esm",
    "build:umd": "rollup --config"
  },
  "dependencies": {
    "camel-case": "^4.1.1",
    "fusion-path": "0.0.1",
    "lower-case": "^2.0.1",
    "no-case": "^3.0.4",
    "param-case": "^3.0.4",
    "pascal-case": "^3.1.1",
    "upper-case": "^2.0.1"
  },
  "devDependencies": {
    "tslib": "^2.4.1"
  }
}
